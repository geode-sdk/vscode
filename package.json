{
  	"name": "geode",
  	"displayName": "Geode",
  	"description": "Utilities for the Geode Geometry Dash modding framework",
  	"version": "1.6.0",
  	"engines": {
  	  	"vscode": "^1.72.0"
  	},
  	"author": {
  	  	"name": "Geode Team",
  	  	"url": "https://github.com/geode-sdk"
  	},
  	"publisher": "GeodeSDK",
  	"categories": [
  	  	"Other"
  	],
  	"repository": {
  	  	"url": "https://github.com/geode-sdk/vscode"
  	},
  	"activationEvents": [
  	  	"onLanguage:cpp",
  	  	"workspaceContains:**/mod.json"
  	],
  	"galleryBanner": {
  	  	"theme": "dark",
  	  	"color": "#24222B"
  	},
  	"main": "./dist/extension.js",
  	"contributes": {
  	  	"commands": [
  	  	  	{
  	  	  	  	"title": "Geode: Launch Geometry Dash",
  	  	  	  	"command": "geode.launchGD"
  	  	  	},
  	  	  	{
  	  	  	  	"title": "Geode: Open Sprite Browser",
  	  	  	  	"command": "geode.openSpriteBrowser"
  	  	  	},
  	  	  	{
  	  	  	  	"title": "Geode: Publish Mod",
  	  	  	  	"command": "geode.publishMod"
  	  	  	}
  	  	],
  	  	"keybindings": [
  	  	  	{
  	  	  	  	"command": "geode.openSpriteBrowser",
  	  	  	  	"key": "Ctrl+Alt+B"
  	  	  	}
  	  	],
		"languages": [{
			"id": "bro",
			"aliases": ["Bro"],
			"extensions": [".bro"],
			"configuration": "./src/broma/language-configuration.json"
		}],
		"grammars": [
			{
				"injectTo": [
					"source.cpp",
					"source.hpp",
					"source.c",
					"source.h"
				],
				"scopeName": "source.geode-flalert-literal",
				"path": "./assets/flalert-colors.json"
			},
			{
				"language": "bro",
				"scopeName": "source.bro",
				"path": "./src/broma/bro.tmLanguage.json"
			},
			{
				"injectTo": [
					"source.bro"
				],
				"scopeName": "source.bro-extra",
				"path": "./src/broma/bro.inject.json"
			}
		],
		"configurationDefaults": {
			"editor.tokenColorCustomizations": {
				"textMateRules": [
					{
						"scope": "geode.flalert.blue",
						"settings": { "foreground": "#4a52e1" }
					},
					{
						"scope": "geode.flalert.green",
						"settings": { "foreground": "#40e348" }
					},
					{
						"scope": "geode.flalert.aqua",
						"settings": { "foreground": "#60abef" }
					},
					{
						"scope": "geode.flalert.cyan",
						"settings": { "foreground": "#32c8ff" }
					},
					{
						"scope": "geode.flalert.yellow",
						"settings": { "foreground": "#ffff00" }
					},
					{
						"scope": "geode.flalert.orange",
						"settings": { "foreground": "#ffa54b" }
					},
					{
						"scope": "geode.flalert.red",
						"settings": { "foreground": "#ff5a5a" }
					},
					{
						"scope": "geode.flalert.pink",
						"settings": { "foreground": "#ff00ff" }
					}
				]
			}
		},
  	  	"configuration": {
  	  	  	"title": "Geode",
  	  	  	"properties": {
  	  	  	  	"geode.geodeCliPath": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"default": "",
  	  	  	  	  	"description": "Path to the Geode CLI executable"
  	  	  	  	},
  	  	  	  	"geode.geodeSdkPath": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"default": "",
  	  	  	  	  	"description": "Path to the Geode SDK"
  	  	  	  	},
  	  	  	  	"geode.textureQuality": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"enum": [
  	  	  	  	  	  	"High",
  	  	  	  	  	  	"Medium",
  	  	  	  	  	  	"Low"
  	  	  	  	  	],
  	  	  	  	  	"default": "Medium",
  	  	  	  	  	"description": "Quality of the graphics to show on the Sprite Browser"
  	  	  	  	},
  	  	  	  	"geode.defaultSpriteShowCount": {
  	  	  	  	  	"type": "integer",
  	  	  	  	  	"default": 350,
  	  	  	  	  	"description": "How many items to show in the Sprite Browser at once by default"
  	  	  	  	},
  	  	  	  	"geode.showCountIncrement": {
  	  	  	  	  	"type": "integer",
  	  	  	  	  	"default": 250,
  	  	  	  	  	"description": "How many items to add in the Sprite Browser when you click 'Load More'"
  	  	  	  	}
  	  	  	}
  	  	},
  	  	"jsonValidation": [
  	  	  	{
  	  	  	  	"fileMatch": "mod.json",
  	  	  	  	"url": "./assets/mod-info-validation.json"
  	  	  	}
  	  	]
  	},
  	"icon": "assets/geode-circle-no-gradient.png",
  	"scripts": {
  	  	"vscode:prepublish": "npm run package",
  	  	"compile": "webpack",
  	  	"watch": "webpack --watch",
  	  	"package": "webpack --mode production --devtool hidden-source-map",
  	  	"compile-tests": "tsc -p . --outDir out",
  	  	"watch-tests": "tsc -p . -w --outDir out",
  	  	"pretest": "npm run compile-tests && npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts"
  	},
  	"devDependencies": {
  	  	"@types/camelcase": "^5.2.0",
  	  	"@types/glob": "^7.2.0",
  	  	"@types/mocha": "^9.1.1",
  	  	"@types/node": "16.x",
  	  	"@types/object-hash": "^2.2.1",
  	  	"@types/plist": "^3.0.2",
  	  	"@types/semver": "^7.3.13",
  	  	"@types/sharp": "^0.31.0",
  	  	"@types/vscode": "^1.72.0",
  	  	"@typescript-eslint/eslint-plugin": "^5.27.0",
  	  	"@typescript-eslint/parser": "^5.27.0",
  	  	"@vscode/test-electron": "^2.1.3",
  	  	"eslint": "^8.16.0",
  	  	"glob": "^8.0.3",
  	  	"mocha": "^10.0.0",
  	  	"ts-loader": "^9.3.0",
  	  	"typescript": "^4.7.2",
  	  	"webpack": "^5.72.1",
  	  	"webpack-cli": "^4.9.2"
  	},
  	"dependencies": {
  	  	"@vscode/codicons": "^0.0.32",
  	  	"@vscode/webview-ui-toolkit": "^1.1.0",
  	  	"camelcase": "^7.0.0",
  	  	"fuse.js": "^6.6.2",
  	  	"object-hash": "^3.0.0",
  	  	"plist": "^3.0.6",
  	  	"semver": "^7.3.8",
  	  	"sharp": "^0.31.2"
  	}
}
